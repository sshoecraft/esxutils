
SOAP_SRCS=soapC.c stdsoap2.c
#SOAP_OBJS=$(SOAP_SRCS:.c=.o$(SUFFIX))
SOAP_OBJS=$(SOAP_SRCS:.c=.o)

#       wsdl2h -cxo empty.h vimService.wsdl
empty.h:
        touch empty.h

soapC.c soapH.h soapClient.c soapStub.h: empty.h
        soapcpp2 -cCLx $<

$(LIB_OBJS): vim.h ivim.h

$(SOAP_OBJS): soapH.h soapStub.h

soapC.o: soapC.c
        $(CC) $(CFLAGS) $(SOAP_CFLAGS) -c $< -o $@

stdsoap2.o: stdsoap2.c
        $(CC) $(CFLAGS) $(SOAP_CFLAGS) -c $< -o $@

